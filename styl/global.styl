.body {
  reset();
}

.body {
  margin: 0;
  line-height: normal;
  font-family: "Roboto", "Helvetica", "Arial", sans-serif;
  width: 100%;
  color: rgb(49, 51, 50);
  padding-left: 100px;
  padding-right 100px;
  box-sizing: border-box;
}

a {
  text-decoration none;
  color: rgb(49, 51, 50);
}

.visually-hidden {
  position: absolute;
  width: 1px;
  height: 1px;
  margin: -1px;
  padding: 0;
  border: 0;
  clip: rect(0 0 0 0);
  overflow: hidden;
}
